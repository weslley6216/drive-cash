default: &default
  adapter: postgresql
  encoding: unicode
  host: localhost
  port: 5432
  username: postgres
  password: postgres
  pool: <%= ENV.fetch("RAILS_MAX_THREADS") { 5 } %>

test:
  <<: *default
  database: drive_cash_test

development:
  <<: *default
  database: drive_cash_development

production:
  <<: *default
  database: drive_cash_production
